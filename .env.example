# NotebookLM Automation Environment Configuration

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=false
SECRET_KEY=your-secret-key-here

# Selenium Configuration
SELENIUM_HUB_URL=http://selenium-chrome:4444/wd/hub
SELENIUM_TIMEOUT=30
SELENIUM_IMPLICIT_WAIT=10

# NotebookLM Configuration
NOTEBOOKLM_BASE_URL=https://notebooklm.google.com
DEFAULT_WAIT_TIME=60
CONTENT_STABILITY_CHECKS=5
CONTENT_CHECK_INTERVAL=2

# Docker Configuration
COMPOSE_PROJECT_NAME=notebooklm-automation

# Logging
LOG_LEVEL=INFO

# CORS Configuration
CORS_ORIGINS=*

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10

